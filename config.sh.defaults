export SRC_DIR=$HOME/src
export PREFIX=$HOME/openrtp
export SUDO=
export APT="apt-get --allow-unauthenticated"
export MAKE_THREADS_NUMBER=2
export BUILD_TYPE=Release
export BUILD_SUBDIR=build
export OCTOMAP_VERSION=1.8.0
export DYNAMORIO_VERSION=Linux-6.2.0-2
export DYNAMORIO_RELEASE=release_6_2_0
export ENABLE_ASAN=0
export IS_VIRTUAL_BOX=0
export ROBOT_HOSTNAME=localhost
export VERBOSE_SCRIPT=0
export INTERNAL_MACHINE=0
export COMPILER_CPP=/usr/bin/g++
export COMPILER_CC=/usr/bin/gcc
export USE_CLANG=0
export BUILD_GOOGLE_TEST=ON
export GITHUB_SSH=0
export PACKAGES="OpenRTM-aist openhrp3 pcl octomap hrpsys-base HRP2 HRP2KAI HRP5P sch-core state-observation hmc2 hrpsys-humanoid hrpsys-private hrpsys-state-observation choreonoid"
